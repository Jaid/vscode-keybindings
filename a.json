{
  "include": [
    "../../../src/**/*.ts"
  ],
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "composite": true,
    "baseUrl": "../../..",
    "outDir": "../../../dist/ts",
    "newLine": "lf",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "es2022",
    "lib": [
      "esnext"
    ],
    "paths": {
      "src/*": [
        "src/*"
      ],
      "lib/*": [
        "src/lib/*"
      ]
    }
  }
}