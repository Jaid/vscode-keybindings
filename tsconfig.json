{
  "extends": "tsconfig-jaid/base",
  "include": [
    "src/**/*.js",
    "src/**/*.ts",
    ".github/actions/**/*.js",
    ".github/actions/**/*.ts"
  ],
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "baseUrl": "."
  },
  "ts-node": {
    "esm": true,
    "compilerOptions": {
      "module": "NodeNext",
      "target": "ESNext"
    }
  }
}